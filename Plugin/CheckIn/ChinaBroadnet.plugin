[URL Rewrite]


[Script]
# Cookie获取
http-request https://wx.10099.com.cn/contact-web/api/busi/qryUserInfo script-path=https://github.com/wuhuhuuuu/study/raw/main/Scripts/ChinaBroadnet/ChinaBroadnet.cookie.js, requires-body=true, timeout=10, tag=中国广电(Cookie), img-url=https://raw.githubusercontent.com/liushiqun/Loon/main/PNG/ChinaBroadnet.png
# 定时查询
cron "3 0 * * *" script-path=https://github.com/wuhuhuuuu/study/raw/main/Scripts/ChinaBroadnet/ChinaBroadnet.js, timeout=10, tag=中国广电(余额), img-url=https://raw.githubusercontent.com/liushiqun/Loon/main/PNG/ChinaBroadnet.png

[MITM]
hostname = wx.10099.com.cn
